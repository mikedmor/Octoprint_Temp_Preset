<!-- ko allowBindings: true -->
<div class="jog-panel" id="tempPresetControl" style="display: none;">
    <h1>Temperature Profiles</h1>
    <div data-bind="foreach: temppreset_profiles">
        <div>
            <span data-bind="text: name" style="font-size: 15px;">Default (before bind)</span>
            <div style="float:right">
                <button class="btn control-box" data-bind="click: $parent.loadBoth, text: 'Both (' + tool_temp.toString() + '/' + bed_temp.toString() + ')'">Both</button>
                <button class="btn control-box" data-bind="click: $parent.loadTool, text: 'Tool (' + tool_temp.toString() + ')' ">Tool</button>
                <button class="btn control-box" data-bind="click: $parent.loadBed, text: 'Bed (' + bed_temp.toString() + ')'">Bed</button>
            </div>
        </div>
        <hr style="margin: 5px">
    </div>
</div>
<!-- /ko -->
